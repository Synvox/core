export type Collection<T> = {
  meta: {
    page: number;
    limit: number;
    hasMore: boolean;
    '@url': string;
    '@links': {
      count: string;
      ids: string;
      previousPage?: string;
      nextPage?: string;
    };
  };
  data: T[];
};

export namespace Test {
  export type User = {
    id: number;
    email: string;
    createdAt: string;
    updatedAt: string;
    deletedAt: string | null;
    '@url': string;
    '@links': {
    };
  };
}

